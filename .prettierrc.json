
{
  "$schema": "https://json.schemastore.org/prettierrc",
  "trailingComma": "all",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": false,
  "experimentalTernaries": true,
  "printWidth": 120,
  "arrowParens": "always",
  "proseWrap": "always",
  "vueIndentScriptAndStyle": true,
  "singleAttributePerLine": true
}